{
  "name": "tailwindx",
  "version": "1.0.2",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "start": "node -r ts-node/register ./src/index.ts",
    "dev": "nodemon",
    "build": "tsc",
    "prebuild": "rm -Rf ./dist",
    "test": "jest",
    "reinstall": "rm -Rf dist node_modules package-lock.json && npm i"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/node": "^14.0.14",
    "jest": "^26.6.3",
    "nodemon": "^2.0.4",
    "ts-jest": "^26.5.5",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  }
}
